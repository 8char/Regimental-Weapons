AtlasUI.Loader()
    :setName("Regimental Weapons")
    :setAcronym("RegWeapons")
    :setDirectory("reg_weapons")
    :setColor(AtlasUI.Theme.Primary)
    :load("network", {
        client = ATLASUI_CLIENT,
        server = ATLASUI_SERVER
    })
    :load("classes", ATLASUI_SHARED, true)
    :load("core/ui", ATLASUI_CLIENT)
    :loadFile("core/main", ATLASUI_SERVER)
:done()


if (AtlasUI) then
	Load()
else
	hook.Add("AtlasUI.Loaded", "RegimentalWeapons", Load)
end